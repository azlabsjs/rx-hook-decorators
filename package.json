{
  "name": "@azlabsjs/rx-hook-decorators",
  "version": "0.1.0",
  "description": "Provides decorators for ES6 compatible classes for rx-hooks library ",
  "type": "module",
  "main": "./dist/index.mjs",
  "typings": "./dist/types/index.d.ts",
  "module": "./dist/index.mjs",
  "files": [
    "dist/**/*"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "rimraf ./dist && npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir dist --extensions \".ts,.tsx\" --source-maps inline --out-file-extension '.mjs'",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "default": "./dist/index.mjs"
    }
  },
  "keywords": [
    "ES6",
    "Decorator",
    "Rx"
  ],
  "author": "azandrew-sidoine",
  "license": "MIT",
  "dependencies": {
    "@azlabsjs/rx-hooks": "^0.2.1"
  },
  "peerDependencies": {
    "rxjs": "^6.0|^7.8"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.0",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-transform-modules-commonjs": "^7.21.2",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.21.0",
    "@types/jest": "^29.4.0",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "jest": "^29.5.0",
    "jest-watch-typeahead": "^2.2.2",
    "rimraf": "^4.3.1",
    "rxjs": "^7.8.0",
    "ts-jest": "^29.0.5",
    "ts-lib": "^0.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/@azlabsjs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/azlabsjs/rx-hook-decorators"
  }
}
